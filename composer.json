{
  "require": {
    "barrelstrength/sprout-encode-email": "2.0.7",
    "craftcms/cms": "3.6.8",
    "craftcms/commerce": "3.2.17",
    "craftcms/commerce-stripe": "2.3.2.1",
    "craftcms/contact-form": "2.2.7",
    "craftcms/mailgun": "1.4.3",
    "craftcms/redactor": "2.8.5",
    "nystudio107/craft-typogrify": "1.1.18",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
